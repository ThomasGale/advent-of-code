{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "g++ build and debug Run",
        "type": "cppdbg",
        "request": "launch",
        "program": "./out/Run/Run",
        "args": ["2018", "1"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "g++ build active file",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }